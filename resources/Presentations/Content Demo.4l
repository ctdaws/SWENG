                    <Text>
                    da/dt = 12t
                    </Text>
                </Answer>
                <Answer id="a10" answernum="2" correct="false">
                    <Image id="incorrectImage1" x="649" y="389" x2="749" y2="489" path="incorrect.png" start="trigger" />
                    <Audio id="incorrectAudio1" path="INCORRECT.wav" start="trigger" />
                    <Text>
                    da/dt = 6t^2 + 5t
                    </Text>
